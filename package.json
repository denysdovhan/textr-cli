{
  "name": "textr-cli",
  "version": "0.1.0",
  "description": "CLI for Textr",
  "author": {
    "name": "Denys Dovhan",
    "email": "email@denysdovhan.com",
    "url": "denysdovhan.com"
  },
  "bin": {
    "textr": "bin/textr"
  },
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "lint": "eslint cli",
    "start": "babel cli --watch --out-file cli.es5.js ",
    "test": "mocha --compilers js:babel-core/register",
    "tdd": "npm test -- --watch",
    "transpile": "babel cli --out-file cli.es5.js",
    "prepublish": "npm run transpile",
    "clean": "rimraf cli.es5.js",
    "push": "git push --follow-tags",
    "postpublish": "npm-run-all clean push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denysdovhan/textr-cli.git"
  },
  "keywords": [
    "cli",
    "cli-app",
    "textr"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/denysdovhan/textr-cli/issues"
  },
  "homepage": "https://github.com/denysdovhan/textr-cli#readme",
  "devDependencies": {
    "assert": "^1.3.0",
    "babel": "^6.0.15",
    "babel-cli": "^6.0.12",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-preset-es2015": "^6.0.12",
    "eslint": "^1.8.0",
    "mocha": "^2.3.3",
    "npm-run-all": "*",
    "rimraf": "*",
    "typographic-ellipses": "^1.0.11",
    "typographic-quotes": "^1.2.1",
    "typographic-single-spaces": "^1.0.2"
  },
  "dependencies": {
    "get-stdin": "^5.0.0",
    "meow": "^3.5.0",
    "textr": "^0.3.0"
  }
}
